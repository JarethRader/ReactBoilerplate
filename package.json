{
  "name": "react_boilerplate",
  "version": "1.1.0",
  "description": "React boilerplate template",
  "main": "src/index.ts",
  "scripts": {
    "start": "env NODE_ENV=development webpack serve --mode development --config webpack.dev.js",
    "docker": "env NODE_ENV=local webpack-dev-server --host 0.0.0.0 --port 3000 --mode development --config webpack.config.js",
    "dev": "env NODE_ENV=development webpack --mode development --config webpack.dev.js",
    "build": "env NODE_ENV=production webpack --mode production --config webpack.prod.js",
    "analyze": "env NODE_ENV=analyze webpack --mode development --config webpack.dev.js",
    "css": "npx tailwindcss build -i tailwind/tailwind.css -o src/style.css"
  },
  "author": "Jareth Rader <jarethrdev@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.15.0",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^5.2.0",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss-safe-parser": "^5.0.2",
    "react-dev-utils": "^11.0.3",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.24.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.7.3",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "postcss-loader": "^5.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "tailwindcss": "^2.2.7",
    "ts-loader": "^8.0.17"
  }
}
